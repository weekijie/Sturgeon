# Sturgeon AI Service - Environment Variables
# Copy this file to .env and fill in your values

# Gemini API Key (required for agentic orchestration mode)
# Get yours at: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini model to use for orchestration (optional, defaults to gemini-3-flash-preview)
# Options: gemini-3-flash-preview, gemini-2.5-flash, gemini-2.5-pro
# GEMINI_MODEL=gemini-3-flash-preview

# AMD GPU environment (required for ROCm)
TORCH_ROCM_AOTRITON_ENABLE_EXPERIMENTAL=1
