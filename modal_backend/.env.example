# Modal Secrets (create via: modal secret create <name> KEY=value)

# Required: Gemini API for orchestrator
modal secret create gemini-api-key GEMINI_API_KEY=your_gemini_api_key_here

# Required: HuggingFace token for gated MedGemma model
modal secret create huggingface-token HF_TOKEN=your_huggingface_token_here
# Get your token from: https://huggingface.co/settings/tokens
# Also accept the license at: https://huggingface.co/google/medgemma-1.5-4b-it

# Optional: Specify Gemini model
GEMINI_MODEL=gemini-3-flash-preview

# Optional: Disable MedSigLIP triage
# DISABLE_MEDSIGLIP=1
